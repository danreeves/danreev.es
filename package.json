{
  "name": "danreev.es",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "skelly build --production",
    "now-start": "serve converted-html --clean",
    "start": "skelly serve"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-eslint": "6.1.2",
    "eslint": "3.3.1",
    "eslint-config-airbnb": "10.0.1",
    "eslint-plugin-flowtype": "2.7.1",
    "eslint-plugin-import": "1.13.0",
    "eslint-plugin-jsx-a11y": "2.1.0",
    "eslint-plugin-react": "6.1.0",
    "rollup": "^0.40.0",
    "skelly": "2.0.0-rc.8"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb",
    "plugins": [
      "flowtype"
    ],
    "rules": {
      "flowtype/define-flow-type": 1,
      "flowtype/require-parameter-type": 1,
      "flowtype/require-return-type": [
        1,
        "always",
        {
          "annotateUndefined": "never"
        }
      ],
      "flowtype/space-after-type-colon": [
        1,
        "always"
      ],
      "flowtype/space-before-type-colon": [
        1,
        "always"
      ],
      "flowtype/type-id-match": [
        1,
        "^([A-Z][a-z0-9]+)+Type$"
      ],
      "flowtype/use-flow-type": 1,
      "flowtype/valid-syntax": 1,
      "semi": [
        1,
        "always"
      ],
      "indent": [
        1,
        4
      ],
      "no-console": 0,
      "padded-blocks": 0,
      "no-empty": 0
    },
    "settings": {
      "flowtype": {
        "onlyFilesWithFlowAnnotation": true
      }
    }
  },
  "dependencies": {
    "baffle": "0.3.5",
    "concaveman": "^1.1.1",
    "counter.js": "1.0.8",
    "d3": "^4.4.0",
    "google-maps": "3.2.1",
    "haversine": "^1.0.2",
    "lodash.chunk": "4.2.0",
    "lodash.flatten": "^4.4.0",
    "serve": "https://github.com/danreeves/serve/releases/download/666/serve.tar.gz",
    "whatwg-fetch": "1.0.0"
  }
}
