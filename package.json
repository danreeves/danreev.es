{
  "name": "danreev.es",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "skelly build --production",
    "start": "skelly serve"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "skelly": "*"
  },
  "eslintConfig": {
    "extends": "airbnb",
    "rules": {
      "semi": [
        1,
        "always"
      ],
      "indent": [
        1,
        4
      ],
      "space-before-function-paren": [
        1,
        {
          "anonymous": "always",
          "named": "always"
        }
      ],
      "no-console": 0,
      "padded-blocks": 0,
      "no-empty": 0
    }
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ],
    "env": {
      "production": {
        "plugins": [
          "transform-remove-console",
          "transform-remove-debugger"
        ]
      }
    }
  },
  "dependencies": {
    "baffle": "0.3.5"
  }
}
